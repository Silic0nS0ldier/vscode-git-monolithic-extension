    "version": "0.0.0",
    "type": "module",
    "types": "./dist/main.d.ts",
    "exports": {
        "./api/repository/ignore/check/parser": {
            "import": "./dist/api/repository/ignore/check/parser.js",
            "types": "./dist/api/repository/ignore/check/parser.d.ts"
        },
        "./api/version": {
            "import": "./dist/api/version/mod.js",
            "types": "./dist/api/version/mod.d.ts"
        },
        "./api/rev-parse/git-dir": {
